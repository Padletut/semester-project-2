{
  "type": "module",
  "devDependencies": {
    "@eslint/js": "^9.23.0",
    "@playwright/test": "^1.51.1",
    "@types/node": "^22.13.14",
    "cpx": "^1.5.0",
    "eslint": "^9.23.0",
    "eslint-plugin-html": "^8.1.2",
    "globals": "^16.0.0",
    "husky": "^8.0.0",
    "jsdoc": "^4.0.4",
    "prettier": "^3.5.3",
    "sass": "^1.86.0",
    "vite": "^6.2.4",
    "vitest": "^3.1.1"
  },
  "lint-staged": {
    "*.js": "eslint --fix",
    "*.{js,css,md}": "prettier --write",
    "*.html": "prettier --write"
  },
  "scripts": {
    "start": "vite",
    "build": "vite build && postcss dist/assets/*.css --use autoprefixer -d dist/assets/ && cpx \"node_modules/bootstrap-icons/font/fonts/**/*\" dist/assets/fonts && cpx \"src/*.html\" dist/",
    "lint-staged": "npx lint-staged",
    "serve": "vite preview",
    "prepare": "husky install",
    "test": "vitest"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "autoprefixer": "^10.4.21",
    "bootstrap": "^5.3.5",
    "bootstrap-icons": "^1.11.3",
    "postcss-cli": "^11.0.1"
  }
}
